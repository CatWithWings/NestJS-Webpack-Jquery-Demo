{
  "name": "first-demo",
  "version": "0.0.1",
  "description": "first demo",
  "author": "Cat",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "Android >= 3.2",
    "Firefox >= 20",
    "iOS 7"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start:dev": "concurrent --kill-others \"nodemon\" \"npm run start:html\"",
    "start:html": "rimraf dist/static && webpack --watch --config config/webpack.dev.js",
    "start:build": "rimraf dist && tsc && webpack --config config/webpack.prod.js",
    "prestart:prod": "npm run start:build",
    "start:prod": "cross-env NODE_ENV=production node dist/server/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fortawesome/fontawesome": "^1.1.8",
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/typeorm": "^5.2.0",
    "@types/express-handlebars": "^0.0.30",
    "bootstrap": "4.1.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "express-handlebars": "^3.0.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.24.0",
    "mongoose": "^5.2.14",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "serviceworker-webpack-plugin": "^1.0.1",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-merge-sibling-variables": "^6.9.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-preset-minify": "^0.4.3",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "3.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "jest": "^23.5.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
